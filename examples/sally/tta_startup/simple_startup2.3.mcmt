                          (= cal!content' (ite cs!431 cs_frame cal!content))
                          (= cal!delivery' (ite cs!431 cs!432 cal!delivery))
                          (= cal!flag!0' (or cs!431 cal!flag!0))
                          (= cal!flag!1' false)
