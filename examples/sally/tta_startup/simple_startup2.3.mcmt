;; Message enum 
(define-constant cs_frame 0)
(define-constant i_frame 1)

;; PC enum 
(define-constant init 0)
(define-constant listen 1)
(define-constant coldstart 2)
(define-constant active 3)

;; Times
(define-constant slot_time 3)

;; State type (all TCCs added to the assumptions)
(define-state-type state_type (
      (cal!content Real)
      (cal!delivery Real)
      (cal!flag!0 Bool)
      (cal!flag!1 Bool)
      (cal!flag!2 Bool)
      (cal!origin Real)
      (cal!send Real)
      (pc!0 Real)
      (pc!1 Real)
      (pc!2 Real)
      (slot!0 Real)
      (slot!1 Real)
      (slot!2 Real)
      (time Real)
      (time_out!0 Real)
      (time_out!1 Real)
      (time_out!2 Real)
      (propagation Real)
))



;; Initial states
(define-states initial_states state_type 
        (and
          (= time 0)
          (= pc!0 init)
          (= pc!1 init)
          (= pc!2 init)
          (and (< time time_out!0) (< time_out!0 30))
          (and (< time time_out!1) (< time_out!1 30))
          (and (< time time_out!2) (< time_out!2 30))
          (= cal!content i_frame)
          (= cal!delivery 0)
          (= cal!flag!0 false)
          (= cal!flag!1 false)
          (= cal!flag!2 false)
          (= cal!origin 1)
          (= cal!send 0)
	  (and (< 0 propagation) (< propagation (/ slot_time 2))))
)

;; Transition relation
(define-transition transition state_type 
