                          (= cal!delivery' (ite cs!431 cs!432 cal!delivery))
                          (= cal!flag!0' false)
                          (= cal!flag!1' (or cs!431 cal!flag!1))
                          (= cal!flag!2' (or cs!431 cal!flag!2))
                          (= cal!origin' (ite cs!431 1 cal!origin))
                          (= cal!send' (ite cs!431 time cal!send))
                          cs!441)
                        (and
                          cs!452
